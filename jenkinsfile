pipeline {
    agent any

    stages {
        stage ('GetProject') {
            steps {
                git 'https://github.com/grifinmi/michaelspetition'
            }
        }
        stage ('clean') {
            steps {
                sh 'mvn clean'
            }
        }
        stage ('Package') {
            steps {
                sh 'mvn package'
            }
        }
    }
    post {
        success {
            archiveArtifacts allowEmptyArchive: true,
                artifacts: '**/michaelspetition*.war' 
        }
    }
}
