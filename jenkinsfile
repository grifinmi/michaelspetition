pipeline {
    agent any

    stages {
        stage ('GetProject') {
            steps {
                git 'https://github.com/grifinmi/michaelspetition'
            }
        }
        stage ('build') {
            steps {
                sh 'mvn clean:clean'
                sh 'mvn dependency:copy-dependencies'
                sh "mvn package"
            }
        }
//         stage ('Exec') {
//             steps {
//                 sh 'mvn exec:java'
//             }
//         }
        stage ('Package') {
            steps {
                sh 'mvn package'
            }
        }
    }
    post {
        success {
            archiveArtifacts allowEmptyArchive: true,
                artifacts: '**/michaelspetition*.war' 
        }
    }
}
